{
    "name": "nest-typescript-starter",
    "version": "1.0.0",
    "description": "Nest TypeScript starter repository",
    "license": "MIT",
    "scripts": {
        "start": "node index.js",
        "start:watch": "nodemon",
        "prestart:prod": "tsc",
        "start:prod": "node dist/main.js",
        "migrations:run": "ts-node ./node_modules/.bin/typeorm migrations:run",
        "migrations:revert": "ts-node ./node_modules/.bin/typeorm migrations:revert",
        "migrations:generate": "ts-node ./node_modules/.bin/typeorm migrations:generate -n ",
        "lint": "tslint 'src/**/*.ts'",
        "lint:fix": "tslint --fix 'src/**/*.ts'"
    },
    "dependencies": {
        "@nest-middlewares/cors": "^1.0.2",
        "@nestjs/common": "^4.5.0",
        "@nestjs/core": "^4.5.0",
        "@nestjs/microservices": "^4.5.0",
        "@nestjs/swagger": "^1.1.4",
        "@nestjs/testing": "^4.5.0",
        "@nestjs/typeorm": "^2.0.0",
        "@nestjs/websockets": "^4.5.0",
        "bcrypt": "^3.0.4",
        "class-transformer": "^0.1.9",
        "class-validator": "0.7.3",
        "jsonwebtoken": "^8.1.0",
        "passport": "^0.4.0",
        "passport-jwt": "^3.0.1",
        "pg": "^7.4.1",
        "redis": "^2.7.1",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.4.0",
        "typeorm": "^0.1.21",
        "typescript": "^2.5.2",
        "typescript-optional": "^1.5.0"
    },
    "devDependencies": {
        "@types/node": "^8.0.28",
        "nodemon": "^1.12.1",
        "ts-node": "^3.3.0",
        "tslint": "^5.9.1",
        "tslint-config-airbnb": "^5.4.2"
    }
}
